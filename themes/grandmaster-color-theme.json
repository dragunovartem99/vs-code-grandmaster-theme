{
  "name": "grandmaster",
  "colors": {
    // Base colors
    "foreground": "#999",
    "focusBorder": "#FFBC47",
    "disabledForeground": "#888",
    "icon.foreground": "#EEE",

    // Text colors
    "textLink.foreground": "#FFBC47",

    // Buttons
    "button.background": "#FFBC47",
    "button.foreground": "#111",

    // Lists
    "list.activeSelectionBackground": "#FFBC47",
    "list.activeSelectionForeground": "#111",
    "list.activeSelectionIconForeground": "#111",
    "list.errorForeground": "#E75454",

    // Activity bar
    "activityBar.background": "#111",
    "sideBarSectionHeader.background": "#111",
    "activityBarBadge.background": "#E68046",

    // Side bar
    "sideBar.background": "#111",

    // Minimap
    "minimap.errorHighlight": "#E75454",

    // Tabs
    "editorGroupHeader.tabsBackground": "#111",
    "tab.activeBackground": "#1A1A1A",
    "tab.inactiveBackground": "#111",

    // Editor
    "editor.background": "#1A1A1A",
    "editor.foreground": "#AAA",
    "editorCursor.foreground": "#FFBC47",
    "editor.selectionBackground": "#FFFFFF33",
    "editorError.foreground": "#E75454",

    // Status bar
    "statusBar.background": "#111",
    "statusBar.foreground": "#999"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4A4A4A"
      }
    },

    // JS

    {
      "name": "Numbers, nulls, booleans",
      "scope": [
        "constant.numeric.decimal",
        "constant.language.null",
        "constant.language.boolean"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFBC47"
      }
    },

    {
      "name": "Strings",
      "scope": [
        "string.quoted.single",
        "string.quoted.double",
        "meta.template.expression",
        "string.template"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#DDD"
      }
    },

    {
      "name": "Template expression dollar",
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#FFBC47"
      }
    },

    {
      "name": "Functions",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#FFBC47"
      }
    },

    {
      "name": "Arrow",
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#E68046"
      }
    },

    {
      "name": "Return Keyword",
      "scope": ["keyword.control.flow"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#DDD"
      }
    },

    {
      "name": "This Keyword",
      "scope": ["variable.language.this"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E68046"
      }
    },

    {
      "name": "Variables",
      "scope": ["variable.other.constant", "variable.other.readwrite"],
      "settings": {
        "foreground": "#FFF"
      }
    },

    {
      "name": "Variable parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#F7D59B"
      }
    },

    {
      "name": "Prop, method",
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#CCC"
      }
    },

    // HTML

    {
      "name": "Tag names",
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#7A7A7A"
      }
    },

    {
      "name": "Attribute keys",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#FFBC47"
      }
    },

    // CSS

    {
      "name": "CSS Keywords",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#E68046"
      }
    },

    {
      "name": "CSS Numbers",
      "scope": ["constant.numeric.css"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFBC47"
      }
    },

    {
      "name": "CSS Functions",
      "scope": ["support.function.misc"],
      "settings": {
        "foreground": "#F7D59B"
      }
    },

    {
      "name": "CSS and SCSS Classes",
      "scope": [
        "entity.other.attribute-name.class.css",
        "punctuation.definition.entity.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#FFF"
      }
    },

    {
      "name": "SCSS Amp",
      "scope": ["entity.name.tag.reference.scss"],
      "settings": {
        "foreground": "#FFF"
      }
    }
  ]
}
